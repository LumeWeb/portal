{
    "plugins": {
        "commit-analyzer": {
            "name": "default@^1.0.0"
        },
        "ci-condition": {
            "name": "github",
            "options": {
                "defaultBranch": "*"
            }
        },
        "changelog-generator": {
            "name": "default",
            "options": {
                "emojis": "true"
            }
        },
        "provider": {
            "name": "git",
            "options": {
                "default_branch": "develop",
                "tagger_email": "gitea@git.lumeweb.com",
                "auth": "ssh"
            }
        }
    }
}
