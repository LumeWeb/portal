{
    "plugins": {
        "commit-analyzer": {
            "name": "default@^1.0.0"
        },
        "ci-condition": {
            "name": "github",
            "options": {
                "defaultBranch": "*"
            }
        },
        "changelog-generator": {
            "name": "default",
            "options": {
                "emojis": "true"
            }
        },
        "provider": {
            "name": "github"
        }
    }
}
